<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.stiki.todolist">

    <application
        android:allowBackup="true"
        android:icon="@drawable/todo_list"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TodoList">
        <activity android:name=".view.activity.TambahActivity"></activity>
        <activity android:name=".view.activity.DaftarActivity" />
        <activity android:name=".view.activity.LoginActivity"></activity>
        <activity android:name=".view.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>